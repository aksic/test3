+package org.gs4tr.projectdirector.migration;
+
+import static junit.framework.Assert.assertTrue;
+
+import javax.annotation.Resource;
+
+import org.gs4tr.foundation.modules.migration.DefaultMigrator;
+import org.gs4tr.foundation.modules.migration.Migrator;
+import org.gs4tr.projectdirector.model.test.groovy.annotations.TestCase;
+import org.gs4tr.projectdirector.model.test.groovy.annotations.TestSuite;
+import org.junit.Test;
+import org.springframework.jdbc.support.rowset.SqlRowSet;
+
+import junit.framework.Assert;
+
+@TestSuite("pd540")
+public class Migration540to550Test extends AbstractMigrationTest {
+
+    @Resource(name = "m540to550")
+    private DefaultMigrator _m540to550;
+
+    @Test
+    @TestCase("addProjectIdToTarget")
+    public void addProjectIdToTargetTest() {
+	// Submission 1
+	Long projectId = getJdbcTemplate().queryForObject("SELECT PROJECT_ID FROM PD_TARGET WHERE TARGET_ID = 1",
+		Long.class);
+	Assert.assertEquals(Long.valueOf(9), projectId);
+
+	projectId = getJdbcTemplate().queryForObject("SELECT PROJECT_ID FROM PD_TARGET WHERE TARGET_ID = 2",
+		Long.class);
+	Assert.assertEquals(Long.valueOf(9), projectId);
+
+	// Submission 10
+	projectId = getJdbcTemplate().queryForObject("SELECT PROJECT_ID FROM PD_TARGET WHERE TARGET_ID = 10",
+		Long.class);
+	Assert.assertEquals(Long.valueOf(19), projectId);
+
+	projectId = getJdbcTemplate().queryForObject("SELECT PROJECT_ID FROM PD_TARGET WHERE TARGET_ID = 11",
+		Long.class);
+	Assert.assertEquals(Long.valueOf(19), projectId);
+
+    }
+
+    @Test
+    @TestCase("baseDataSet")
+    public void projectIdInTargetTableTest() {
+	SqlRowSet column = getJdbcTemplate().queryForRowSet(
+		"SELECT COLUMN_NAME FROM INFORMATION_SCHEMA.COLUMNS WHERE TABLE_SCHEMA=DATABASE() AND TABLE_NAME= 'PD_TARGET' AND COLUMN_NAME='PROJECT_ID'");
+	assertTrue(column.next());
+    }
+
+    @Override
+    protected String getInitialStructureFileName() {
+	return "pd540/pd540-create.sql";
+    }
+
+    @Override
+    protected Migrator getMigrator() {
+	return _m540to550;
+    }
+
+}
